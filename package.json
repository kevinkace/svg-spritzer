{
  "name": "svg-spritzer",
  "version": "0.2.0",
  "description": "SVG spriting the way I want it.",
  "main": "index.js",
  "scripts": {
    "test": "npm run lint && mocha",
    "lint": "eslint .",
    "publish-please": "publish-please",
    "prepublish": "publish-please guard"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kevinkace/svg-spritzer.git"
  },
  "keywords": [
    "svg",
    "sprite"
  ],
  "author": "Kevin Cameron",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kevinkace/svg-spritzer/issues"
  },
  "homepage": "https://github.com/kevinkace/svg-spritzer#readme",
  "dependencies": {
    "eslint": "^5.1.0",
    "globule": "^1.1.0",
    "svg-parser": "^1.0.5"
  },
  "devDependencies": {
    "assert": "^1.4.1",
    "mocha": "^5.2.0",
    "publish-please": "^3.2.0"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "rules": {
      "eol-last": [
        "error",
        "always"
      ],
      "no-console": 0,
      "radix": [
        "warn",
        "always"
      ],
      "semi": [
        "error",
        "always"
      ],
      "no-unused-vars": [
        "warn",
        {
          "vars": "all",
          "args": "after-used",
          "ignoreRestSiblings": false
        }
      ]
    }
  }
}
